{
  "name": "react-with-cordova",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "cordova-browser": "^6.0.0",
    "cordova-ios": "^5.0.1",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "react-scripts": "3.1.2"
  },
  "scripts": {
    "start": "npm run remove:www && react-scripts start",
    "build": "npm run remove:www && react-scripts build",
    "build:cordova": "npm run remove:www && react-scripts build && mv build www",
    "test": "react-scripts test",
    "eject": "npm run remove:www && react-scripts eject",
    "remove:www": "rm -rf www"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "browser",
      "ios"
    ]
  }
}